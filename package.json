{
  "name": "KiwiLauncher",
  "version": "0.5.0",
  "description": "kiwi launcher is a free, open source and community based game launcer",
  "homepage": "oxey405.com/kiwilauncher",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist:linux": "electron-builder --linux",
    "dist:win": "electron-builder --win"
  },
  "keywords": [
    "kiwilauncher",
    "game",
    "launcher",
    "free",
    "open-source"
  ],
  "author": "Oxey405 <Oxey405@gmail.com> (https://oxey405.com)",
  "license": "GPL v4",
  "devDependencies": {
    "electron": "^12.0.2",
    "electron-builder": "^22.9.1"
  },
  "build": {
    "appId": "com.oxeygames.kiwilauncher",
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "ressources/logo.ico"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage",
        "snap"
      ],
      "icon": "ressources/logo256x.png",
      "category": "other"
    }
  }
}
